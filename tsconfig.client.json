{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "webworker",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "commonjs",
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "baseUrl": "src",
  "include": [
    "src"
  ]
}
