{
    "name": "registrum-banner-data",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "tsc",
        "lint": "eslint \"**/*.{js,ts}\"",
        "lint:fix": "npm run lint -- --fix",
        "start": "node dist/index.js",
        "test": "nyc --reporter=lcov --reporter=text mocha -r ts-node/register src/tests/**/*.spec.ts",
        "watch": "concurrently -n ts,node \"npm run watch:ts\" \"npm run watch:node\"",
        "watch:node": "nodemon --watch dist dist/index.js",
        "watch:test": "npm run test -- --watch-files src --watch",
        "watch:ts": "tsc -w --preserveWatchOutput"
    },
    "nyc": {
        "exclude": "**/*.spec.ts",
        "include": "src"
    },
    "dependencies": {
        "argparse": "^1.0.10",
        "axios": "^0.19.2",
        "axios-cookiejar-support": "^1.0.0",
        "dotenv": "^8.2.0",
        "mongoose": "^5.9.7",
        "node-cron": "^2.0.3",
        "registrum-banner": "^0.1.0",
        "tough-cookie": "^4.0.0"
    },
    "devDependencies": {
        "@types/argparse": "^1.0.38",
        "@types/chai": "^4.2.11",
        "@types/mocha": "^7.0.2",
        "@types/mongoose": "^5.7.10",
        "@types/node": "^13.11.1",
        "@types/node-cron": "^2.0.3",
        "@types/tough-cookie": "^4.0.0",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "chai": "^4.2.0",
        "concurrently": "^5.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "mocha": "^7.1.1",
        "nodemon": "^2.0.3",
        "nyc": "^15.0.1",
        "prettier": "^2.0.4",
        "ts-node": "^8.8.2",
        "tslib": "^1.11.1",
        "typescript": "^3.8.3"
    }
}
