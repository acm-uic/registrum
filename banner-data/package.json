{
    "name": "registrum-banner-data",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "tsc",
        "lint": "eslint \"**/*.{js,ts}\"",
        "lint:fix": "npm run lint -- --fix",
        "start": "node dist/index.js",
        "test": "jest -i --silent --runInBand",
        "watch": "concurrently -n ts,node \"npm run watch:ts\" \"npm run watch:node\"",
        "watch:node": "nodemon --watch dist dist/index.js",
        "watch:test": "npm run test -- --watch-files src --watch",
        "watch:ts": "tsc -w --preserveWatchOutput"
    },
    "dependencies": {
        "argparse": "^1.0.10",
        "axios": "^0.19.2",
        "axios-cookiejar-support": "^1.0.0",
        "dotenv": "^8.2.0",
        "mongoose": "^5.9.11",
        "node-cron": "^2.0.3",
        "registrum-banner": "^0.1.0",
        "tough-cookie": "^4.0.0"
    },
    "devDependencies": {
        "@types/argparse": "^1.0.38",
        "@types/chai": "^4.2.11",
        "@types/jest": "^25.2.1",
        "@types/mongoose": "^5.7.14",
        "@types/node": "^13.13.4",
        "@types/node-cron": "^2.0.3",
        "@types/tough-cookie": "^4.0.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "chai": "^4.2.0",
        "concurrently": "^5.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^25.5.4",
        "nodemon": "^2.0.3",
        "prettier": "^2.0.5",
        "ts-jest": "^25.4.0",
        "ts-node": "^8.9.1",
        "tslib": "^1.11.1",
        "typescript": "^3.8.3"
    }
}
