{
  "name": "registrum-banner-data",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "node dist/index.js",
    "test": "jest --silent --passWithNoTests",
    "watch": "concurrently -n ts,node \"npm run watch:ts\" \"npm run watch:node\"",
    "watch:node": "nodemon --watch dist dist/index.js",
    "watch:test": "npm run test -- --watch-files src --watch",
    "watch:ts": "tsc -w --preserveWatchOutput"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "axios": "^0.20.0",
    "axios-cookiejar-support": "^1.0.0",
    "dotenv": "^8.2.0",
    "mongoose": "^5.10.7",
    "node-cron": "^2.0.3",
    "registrum-common": "^0.1.0",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.0",
    "@types/chai": "^4.2.12",
    "@types/jest": "^26.0.13",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.6.4",
    "@types/node-cron": "^2.0.3",
    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "chai": "^4.2.0",
    "codecov": "^3.7.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  }
}
