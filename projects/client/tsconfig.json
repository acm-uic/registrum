{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "moduleResolution": "node",
        "jsx": "react",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext",
            "webworker"
        ],
        "strict": true,
        "noEmit": true,
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "resolveJsonModule": true,
        "baseUrl": "src",
        "paths": {
            "@utils/*": [
                "utils/*"
            ],
            "@redux/*": [
                "models/store/*"
            ],
            "@components/*": [
                "components/*"
            ],
            "@styles/*": [
                "styles/*"
            ],
            "@assets/*": [
                "assets/*"
            ],
            "@interfaces/*": [
                "models/interfaces/*"
            ],
            "@pages/*": [
                "pages/*"
            ]
        }
    }
}