    "express-session": "^1.17.1",
    "helmet": "^4.2.0",
    "jest": "^26.6.3",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
