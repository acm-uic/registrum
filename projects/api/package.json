    "private": true,
    "scripts": {
        "build": "tsc",
        "lint": "eslint \"src/**/*.{js,ts}\"",
        "lint:fix": "npm run lint -- --fix",
        "start": "node dist/server.js",
        "test": "jest -i --silent --runInBand",
        "watch": "concurrently -n ts,node \"npm run watch:ts\" \"npm run watch:node\"",
