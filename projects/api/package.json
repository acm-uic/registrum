        "@types/jest": "^25.2.1",
        "@types/mongoose": "^5.7.16",
        "@types/morgan": "^1.9.0",
        "@types/node": "^13.13.5",
        "@types/passport": "^1.0.3",
        "@types/passport-local": "^1.0.33",
        "@types/supertest": "^2.0.9",
        "@types/tough-cookie": "^4.0.0",
        "@typescript-eslint/eslint-plugin": "^2.32.0",
