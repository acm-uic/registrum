        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.1",
        "jest": "^25.5.4",
        "mongoose": "^5.9.13",
        "morgan": "^1.10.0",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "tough-cookie": "^4.0.0",
        "web-push": "^3.4.4",
