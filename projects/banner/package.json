{
  "name": "registrum-banner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "node dist/index.js",
    "test": "jest --silent --passWithNoTests",
    "watch": "concurrently -n ts,node \"npm run watch:ts\" \"npm run watch:node\"",
    "watch:node": "nodemon --watch dist dist/index.js",
    "watch:test": "npm run test -- --watch-files src --watch",
    "watch:ts": "tsc -w --preserveWatchOutput"
  },
  "dependencies": {
    "@bmiddha/webhooks": "^1.1.6",
    "axios": "^0.20.0",
    "axios-cookiejar-support": "^1.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "registrum-common": "^0.1.0",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/ioredis": "^4.17.7",
    "@types/jest": "^26.0.13",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.4",
    "@types/node-fetch": "^2.5.7",
    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "chai": "^4.2.0",
    "codecov": "^3.7.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ioredis": "^4.17.3",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
