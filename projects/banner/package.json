{
  "name": "registrum-banner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "node dist/index.js",
    "test": "jest --silent --passWithNoTests",
    "watch": "concurrently -n ts,node \"npm run watch:ts\" \"npm run watch:node\"",
    "watch:node": "nodemon --watch dist dist/index.js",
    "watch:test": "npm run test -- --watch-files src --watch",
    "watch:ts": "tsc -w --preserveWatchOutput"
  },
  "dependencies": {
    "@bmiddha/webhooks": "^1.1.7",
    "axios": "^0.21.0",
    "axios-cookiejar-support": "^1.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "registrum-common": "^0.1.0",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/chai": "^4.2.14",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/ioredis": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.14",
    "@types/node-fetch": "^2.5.7",
    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "codecov": "^3.8.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "ioredis": "^4.19.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
